class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        out = True
        x_str = str(x)
        for i in range(len(x_str)):
            if (x_str[i] != x_str[-i-1]):
                out = False
                return out
            
        return out
